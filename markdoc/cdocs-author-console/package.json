{
  "name": "cdocs-author-console",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn copy-mock-data && concurrently \"vite build --watch --emptyOutDir false\" \"yarn serve\"",
    "test": "vitest run",
    "serve": "live-server dist --port=8000",
    "build": "tsc -b; vite build; yarn copy-mock-data",
    "prod": "yarn build && open dist/index.html",
    "lint": "eslint .",
    "preview": "vite preview",
    "copy-mock-data": "cp -r ./src/mocks/data.json ./dist/data.json"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitest/ui": "^3.1.1",
    "cdocs-data": "https://s3.amazonaws.com/origin-static-assets/corp-node-packages/master/cdocs-data-v1.0.2.tgz",
    "javascript-time-ago": "^2.5.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-time-ago": "^7.3.3",
    "uuid": "^11.1.0",
    "vitest": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vite-plugin-singlefile": "^2.0.2"
  }
}
