<script>
  var googleClientId = "641478567492-2l5kcdhdl4ab3jfgtk7k8lu5qsn18ond.apps.googleusercontent.com";
  var callbackUrl = "https://a.cl.ly/auth/google_oauth2/callback";
  function initGAuth() {
    gapi.load('auth2', function() {
      // Ready.
      $('.google-login-button').click(function(e) {
        e.preventDefault();
        gapi.auth2.authorize({
          client_id: googleClientId,
          scope: 'email profile',
          response_type: 'code',
          state: `${location.search.slice(1)}&redirect_to=${location.pathname}`
        }, function(response) {
          if (response && !response.error) {
            // google authentication succeed, now post data to server.
            jQuery.ajax({
              type: 'POST',
              url: callbackUrl,
              data: response,
              headers: {
                'X-Requested-With': 'XMLHttpRequest'
              },
              success: function(response) {
                try {
                  if (new URL(response.redirect_to).origin != "https://share.getcloudapp.com" && response.redirect_to) {
                    response.redirect_to += ("?token=" + response.jwt);
                  }
                  window.location = !!response.redirect_to ? response.redirect_to : '/dashboard';
                } catch (err) {
                  window.location = !!response.redirect_to ? response.redirect_to : '/dashboard';
                }
              },
              error: function(request){
                let response = request.responseJSON;
                let errors   = response.errors && response.errors.join('\n');
                let redirect = response.metadata && response.metadata.redirect_to;

                if (errors)
                  window.localStorage.setItem(
                    'flash',
                    JSON.stringify({type: 'danger', message: errors})
                  );

                if (redirect)
                  window.location = redirect;
              }
            });
          } else {
            console.log(response.error)
            // TODO: how to alert the user something went wrong?
          }
        });
      });
    });
  };
</script>
<script src="https://apis.google.com/js/platform.js?onload=initGAuth" async defer></script>

    

    <script>
      $(document).ready(function (params) {
        if(window.location.href.indexOf('#download') != -1) { $('#downloadModal').modal(); }

        var dismissBanner = $('#dismiss-notification-banner')
        if(dismissBanner[0]){
          var broadcastShown = localStorage.getItem("broadcast-dismissed-" + dismissBanner.data().broadcastid)
          if(!broadcastShown){
            $('.notification-hidden-content').hide()
            $('.broadcast-notification-banner').css('display', 'flex');
          }else{
            $('.notification-hidden-content').show()
          }

          dismissBanner.on('click', function(e){
            localStorage.setItem("broadcast-dismissed-" + e.target.dataset.broadcastid, 1)
            $('.broadcast-notification-banner').hide();
            $('.notification-hidden-content').show();
          })
        }
      })
    </script>
